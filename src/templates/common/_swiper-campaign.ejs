<% const tourCardDataAll = [ { link : "/tour#licence", imgPath :
"./assets/images/top/tour-card1.jpg", alt : "ライセンス取得の画像", label :
"ライセンス講習", title : "ライセンス取得", text : "全部コミコミ(お一人様)",
cancelledPrice : "¥56,000", price : "¥46,000" }, { link : "/tour#diving-rental",
imgPath : "./assets/images/top/tour-card2.jpg", alt :
"貸切体験ダイビングの画像", label : "体験ダイビング", title :
"貸切体験ダイビング", text : "全部コミコミ(お一人様)", cancelledPrice :
"¥24,000", price : "¥18,000" }, { link : "/tour#diving-night", imgPath :
"./assets/images/top/tour-card3.jpg", alt : "ナイトダイビングの画像", label :
"体験ダイビング", title : "ナイトダイビング", text : "全部コミコミ(お一人様)",
cancelledPrice : "¥10,000", price : "¥8,000" }, { link : "/tour#diving-fun",
imgPath : "./assets/images/top/tour-card4.jpg", alt :
"貸切ファンダイビングの画像", label : "ファンダイビング", title :
"貸切ファンダイビング", text : "全部コミコミ(お一人様)", cancelledPrice :
"¥20,000", price : "¥16,000" }, ]; %>
<div class="js-swiper-tour swiper-tour">
  <div class="swiper-wrapper swiper-tour__wrapper">
    <% tourCardDataAll.forEach(tourCardData => { %>
    <div class="swiper-slide swiper-tour__swiper-slide">
      <div class="top-tour__card">
        <%- include('./_tour-card', { tourCardData : tourCardData}) %>
      </div>
    </div>
    <% }); %>
  </div>
  <div class="swiper-tour__button-prev u-desktop">
    <div class="button-scroll--left"></div>
  </div>
  <div class="swiper-tour__button-next u-desktop">
    <div class="button-scroll--right"></div>
  </div>
</div>
