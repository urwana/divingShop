<% if (typeof $breadcrumbsData == 'undefined') { $breadcrumbsData = []; } %>

<div class="breadcrumbs">
<% $breadcrumbsData.forEach((breadcrumb, index) => { %>
  <% if (index === $breadcrumbsData.length - 1) { %>
    <span class="breadcrumbs__item-current"><%- breadcrumb.title %></span>
  <% } else { %>
    <a href="<%- breadcrumb.url %>" class="breadcrumbs__item"><%- breadcrumb.title %></a>
  <% } %>
<% }); %>
</div>